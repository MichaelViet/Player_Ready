  sentences:
  - text: "\u0410 \u043E\u0441\u044C \u0456 \u0448\u043A\u043E\u043B\u0430."
    speaker: {fileID: 11400000, guid: eddfe009b49d3484d8dfb5498d6a2f17, type: 2}
    actions: []
  - text: "\u0410\u0433\u043E\u0432!"
    speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
      spriteIndex: 0
      actionType: 1
      coords: {x: 0, y: 0}
      moveSpeed: 0
  - text: "\u0428\u043E \u0442\u0438 \u0433\u043E\u043B\u043E\u0432\u0430?"
    speaker: {fileID: 11400000, guid: eddfe009b49d3484d8dfb5498d6a2f17, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: eddfe009b49d3484d8dfb5498d6a2f17, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
  - text: "\u0422\u0430 \u043D\u0456\u0447\u043E, \u043B\u0430\u0441\u0442 \u0434\u0435\u043D\u044C
      \u0448\u043A\u043E\u043B\u0438. \u0429\u0430 \u0435\u043A\u0437\u0438 \u0437\u0434\u0430\u043C\u043E
      \u0456 \u043A\u0430\u043D\u0456\u043A\u0443\u043B\u0438!"
    speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
  - text: "\u0410\u0410\u0410\u0410, \u0435\u043A\u0437\u0430\u043C\u0435\u043D...."
    speaker: {fileID: 11400000, guid: 755db819ff1a2ea4aa12594b5b3997b8, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
  - text: "\u0422\u0430\u043A \u0434\u043E\u0431\u0440\u0435. \u0433\u043E\u043B\u043E\u0432\u043D\u0435
      \u043D\u0435 \u043F\u0430\u043D\u0456\u043A\u0443\u0432\u0430\u0442\u0438."
    speaker: {fileID: 11400000, guid: eddfe009b49d3484d8dfb5498d6a2f17, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
  - text: "\u042F\u043A \u0442\u0443\u0442 \u043C\u043E\u0436\u043D\u0430 \u043D\u0435
      \u043F\u0430\u043D\u0456\u043A\u0443\u0432\u0430\u0442\u0438? "
    speaker: {fileID: 11400000, guid: 755db819ff1a2ea4aa12594b5b3997b8, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
  - text: "\u0410 \u0437 \u0447\u043E\u0433\u043E \u0435\u043A\u0437\u0430\u043C\u0435\u043D?"
    speaker: {fileID: 11400000, guid: eddfe009b49d3484d8dfb5498d6a2f17, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
  - text: "\u0417 \u043C\u0430\u0442\u0430\u043D\u0430"
    speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
  - text: "\u0410 \u0434\u043E\u0431\u0440\u0435, \u043C\u0430\u0442\u0430\u043D
      \u0442\u043E \u044F \u0437\u043D\u0430\u044E!"
    speaker: {fileID: 11400000, guid: eddfe009b49d3484d8dfb5498d6a2f17, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: 0, y: 0}
      moveSpeed: 0
  - text: "\u041D\u0435 \u0434\u0438\u0432\u043B\u044F\u0447\u0438\u0441\u044C \u043D\u0430
      \u0442\u0435 \u0449\u043E \u044F \u043F\u0440\u043E\u043F\u0443\u0441\u043A\u0430\u0432
      \u0431\u0430\u0433\u0430\u0442\u043E \u0443\u0440\u043E\u043A\u0456\u0432 \u0437
      \u043C\u0430\u0442\u0430\u043D\u043E\u043C \u043F\u0440\u0440\u0431\u043B\u0435\u043C
      \u0443 \u043C\u0435\u043D\u0435 \u043D\u0456\u043A\u043E\u043B\u0438 \u043D\u0435
