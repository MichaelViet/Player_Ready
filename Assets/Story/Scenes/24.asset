  sentences:
  - text: "\u0412\u0435\u0447\u043E\u0440\u0456\u043B\u043E."
    speaker: {fileID: 11400000, guid: 755db819ff1a2ea4aa12594b5b3997b8, type: 2}
    actions: []
    music: {fileID: 8300000, guid: 306579d9bb13af84a85c188eee5c5de1, type: 3}
    sound: {fileID: 0}
    environment: {fileID: 0}
  - text: "\u041C\u0430\u0439\u043A\u043B \u0436\u0438\u0432\u0435 \u0434\u0430\u043B\u0435\u043A\u043E.
      \u041C\u0438 \u043F\u0440\u043E\u0441\u0442\u043E \u0442\u0438\u043D\u044F\u043B\u0438\u0441\u044F
      \u043C\u0456\u0441\u0442\u043E\u043C \u0443\u0436\u0435 \u0431\u0430\u0433\u0430\u0442\u043E
      \u0433\u043E\u0434\u0438\u043D, \u0437\u0440\u0456\u0434\u043A\u0430 \u0437\u0430\u0445\u043E\u0434\u044F\u0447\u0438
      \u0432 \u044F\u043A\u0456\u0441\u044C \u043C\u0430\u0433\u0430\u0437\u0438\u043D\u0438
      \u0430\u0431\u043E \u043F\u0440\u043E\u0441\u0442\u043E \u0434\u0438\u0432\u043B\u044F\u0447\u0438\u0441\u044C
      \u043D\u0430 \u0432\u0456\u0442\u0440\u0438\u043D\u0438."
    speaker: {fileID: 11400000, guid: 755db819ff1a2ea4aa12594b5b3997b8, type: 2}
    actions: []
    music: {fileID: 8300000, guid: 306579d9bb13af84a85c188eee5c5de1, type: 3}
    sound: {fileID: 0}
    environment: {fileID: 0}
  - text: "\u042F\u043A\u0449\u043E \u0432\u0434\u0435\u043D\u044C \u0443 \u0422\u043E\u043A\u0456\u043E
      \u0431\u0443\u043B\u043E \u0449\u0435 \u043F\u043E-\u043B\u0456\u0442\u043D\u044C\u043E\u043C\u0443
      \u0442\u0435\u043F\u043B\u043E, \u0442\u043E \u0432\u0435\u0447\u043E\u0440\u0438
      \u0441\u0442\u0430\u0432\u0430\u043B\u0438 \u0432\u0441\u0435 \u0445\u043E\u043B\u043E\u0434\u043D\u0456\u0448\u0438\u043C\u0438
      \u0456 \u0445\u043E\u043B\u043E\u0434\u043D\u0456\u0448\u0438\u043C\u0438."
    speaker: {fileID: 11400000, guid: 755db819ff1a2ea4aa12594b5b3997b8, type: 2}
    actions: []
    music: {fileID: 8300000, guid: 306579d9bb13af84a85c188eee5c5de1, type: 3}
    sound: {fileID: 0}
    environment: {fileID: 0}
  - text: "\u0425\u043E\u0434\u0456\u043C\u043E \u043D\u0430 \u043C\u0435\u0442\u0440\u043E."
    speaker: {fileID: 11400000, guid: 1bb84b5530fc445409951fc893429ff9, type: 2}
